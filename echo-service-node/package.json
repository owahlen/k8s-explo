{
  "name": "echo-service",
  "version": "1.0.0",
  "description": "Simple echo of incoming requests",
  "type": "module",
  "scripts": {
    "dev": "OTEL_ENABLED=false vite",
    "dev:otel": "OTEL_EXPORTER_OTLP_ENDPOINT='http://localhost:4318' OTEL_METRICS_EXPORTER=otlp OTEL_EXPORTER_OTLP_METRICS_PROTOCOL='http/protobuf' OTEL_METRIC_EXPORT_INTERVAL=20000 OTEL_METRIC_EXPORT_TIMEOUT=20000 OTEL_SEMCONV_STABILITY_OPT_IN='http' OTEL_SERVICE_NAME='echo-service-node' OTEL_SERVICE_NAMESPACE='default' vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "0.64.1",
    "@opentelemetry/instrumentation-express": "^0.54.0",
    "@opentelemetry/instrumentation-http": "^0.205.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "express": "^5.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.1.4",
    "vite": "^7.1.5",
    "vite-plugin-node": "^7.0.0",
    "vitest": "^3.2.4"
  }
}
